---
import ImageWrapper from "@components/astro/util/ImageWrapper.astro";
import type { z } from "astro:content";

import type { caseImageBlock } from "@/content/config";

type CaseImageBlock = z.infer<typeof caseImageBlock>;

interface Props {
  block: CaseImageBlock;
}

const { block } = Astro.props;
const { image } = block;
---

<ImageWrapper src={image.image} altText={image.alt} />
