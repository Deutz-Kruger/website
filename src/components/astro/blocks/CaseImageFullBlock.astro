---
import ImageWrapper from "@components/astro/util/ImageWrapper.astro";
import type { z } from "astro:content";

import type { caseImageFullBlock } from "@/content/config";

type CaseImageFullBlock = z.infer<typeof caseImageFullBlock>;

interface Props {
  block: CaseImageFullBlock;
}

const { block } = Astro.props;
const { image } = block;
---

<ImageWrapper src={image.image} altText={image.alt} />
